    {
        "id": "6e1be725.32a228",
        "type": "function",
        "z": "b43ac849.4051e",
        "name": "calculate pressure",
        "func": "var pressure = (msg.raw_adc * flow.get('sensor_range')) / (4095.0 * flow.get('pressure_conversion'));\n\nmsg.pressure = pressure;\nmsg.payload = pressure.toFixed(flow.get('pressure_decimals'));\nmsg.pressure_label = flow.get('pressure_label');\n\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 890,
        "y": 300,
        "wires": [
            [
                "f9e1faeb.119d68",
                "ae533b1f.9a4388"
            ]
        ]
    },
    {
        "id": "bea0e5c3.54df78",
        "type": "function",
        "z": "b43ac849.4051e",
        "name": "configure gauge",
        "func": "var ui_control = {};\n\nui_control.min = 0;\n\nswitch(flow.get('pressure_unit')) {\n    default:\n    case \"BAR\":\n        ui_control.max = 3;\n        ui_control.seg1 = 1;\n        ui_control.seg2 = 2;\n        flow.set('pressure_label', \"Bar\");\n        flow.set('pressure_decimals', 2);\n    break;\n    case \"PSI\":\n        ui_control.max = 40;\n        ui_control.seg1 = 14.5;\n        ui_control.seg2 = 29.0;\n        flow.set('pressure_label', \"psi\");\n        flow.set('pressure_decimals', 1);\n    break;\n    case \"KPA\":\n        ui_control.max = 300;\n        ui_control.seg1 = 100;\n        ui_control.seg2 = 200;\n        flow.set('pressure_label', \"kPa\");\n        flow.set('pressure_decimals', 0);\n    break;\n}\n\nmsg = {};\nmsg.ui_control = ui_control;\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 920,
        "y": 380,
        "wires": [
            [
                "f9e1faeb.119d68"
            ]
        ]
