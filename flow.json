    {
        "id": "8e0163ae.a8aa8",
        "type": "function",
        "z": "b43ac849.4051e",
        "name": "Setup cloud post",
        "func": "var postEnabled = (flow.get('log_to_cloud'))||false;\nvar interval = flow.get('cloud_logging_interval')||15;\ninterval *= 60000;\nif (postEnabled && flow.get('cloud_logging_url')){\n    var lastPost = flow.get('lastpost')||0;\n\n    if (msg.timestamp - lastPost > interval){\n        flow.set('lastpost' , msg.timestamp);\n        msg.headers = {};\n        msg.headers['Content-type'] = 'application/json';\n        msg.url = flow.get('cloud_logging_url');\n        payload = \n            {\"name\":msg.devicename,\n            \"comment\":flow.get('comment'),\n            \"pressure\":msg.pressure,\n            \"pressure_unit\":msg.pressure_unit};\n        msg.payload = JSON.stringify(payload);\n\n        return msg;\n    }\n}",
