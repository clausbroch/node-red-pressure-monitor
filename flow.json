    {
        "id": "6e1be725.32a228",
        "type": "function",
        "z": "b43ac849.4051e",
        "name": "calculate pressure",
        "func": "var adc_zero_offset = flow.get('adc_zero_offset') || 0;\nvar sensor_min_adc = flow.get('sensor_min_adc') || 0;\nvar sensor_max_adc = flow.get('sensor_max_adc') || 4095;\nvar raw_adc = msg.raw_adc;\nvar pressure = ((raw_adc + adc_zero_offset - sensor_min_adc) * flow.get('sensor_range')) / ((sensor_max_adc - sensor_min_adc) * flow.get('pressure_conversion'));\n\nmsg.pressure = pressure;\nmsg.payload = pressure.toFixed(flow.get('pressure_decimals'));\nmsg.devicename = flow.get('devicename');\nmsg.pressure_label = flow.get('pressure_label');\nmsg.pressure_unit = flow.get('pressure_unit');\n\nflow.set('pressure', pressure);\nflow.set('raw_adc', raw_adc);\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 1290,
        "y": 300,
        "wires": [
            [
                "f9e1faeb.119d68",
                "ae533b1f.9a4388",
                "8e0163ae.a8aa8"
            ]
        ]
    },
    {
        "id": "bea0e5c3.54df78",
        "type": "function",
        "z": "b43ac849.4051e",
        "name": "configure gauge",
        "func": "var ui_control = {};\nvar pressure_label;\nvar pressure_decimals;\nui_control.min = 0;\n\nswitch(flow.get('pressure_unit')) {\n    default:\n    case \"BAR\":\n        ui_control.max = 3;\n        ui_control.seg1 = 1;\n        ui_control.seg2 = 2;\n        pressure_label = \"Bar\";\n        pressure_decimals = 2;\n    break;\n    case \"PSI\":\n        ui_control.max = 40;\n        ui_control.seg1 = 14.5;\n        ui_control.seg2 = 29.0;\n        pressure_label = \"psi\";\n        pressure_decimals = 1;\n    break;\n    case \"KPA\":\n        ui_control.max = 300;\n        ui_control.seg1 = 100;\n        ui_control.seg2 = 200;\n        pressure_label = \"kPa\";\n        pressure_decimals = 0;\n    break;\n}\n        \nflow.set('pressure_label', pressure_label);\nflow.set('pressure_decimals', pressure_decimals);\n\nmsg = {};\nmsg.pressure_label = pressure_label;\nmsg.ui_control = ui_control;\nmsg.devicename = flow.get('devicename');\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 1280,
        "y": 380,
        "wires": [
            [
                "f9e1faeb.119d68"
            ]
        ]
    },
